<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yys.mapper.UserMapper">
    <insert id="insert">

    </insert>
    <select id="login" resultType="com.yys.entity.AiUser">
        select * from ai_user where user_name = #{userName} and user_pwd = #{userPwd}
    </select>
</mapper>